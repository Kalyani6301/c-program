/*pythagareon triples/*
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() 
{
int m,n,o = 0,a=2,limit,b;
  scanf("%d",&limit);
     while (o < limit) 
	 { 
        for ( b=1;b<a;++b)
		 { 
              m= a * a - b* b; 
            n = 2 * a * b; 
            o = a * a + b * b; 
            if (o > limit) 
                break; 
            printf("%d %d %d\n", m,n,o); 
        } 
        a++; 
    }  
    return 0;
}
